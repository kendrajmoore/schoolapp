<h1>Schools</h1>
</br>
</br>
<div class="row justify-content-between">
  {{#each schools}}
    <div class="col-sm-4">
        <div class="schoolindex">
            <a href="/schools/{{this._id}}">
              <h3>{{this.title}}</h3>
            </a>
            <small>{{this.address}}</small>
            <p>{{this.grades}}</p>
            </br>
            <p>Rating: {{this.rating}}</p>
            </br>
        </div>
        <a href="/schools/{{this._id}}">School Information</a>
        <br>
        <!-- <a href="">Review</a> -->


      <hr>
    </div>
  {{/each}}
</div>
